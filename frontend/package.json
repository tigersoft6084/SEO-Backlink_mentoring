{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "set PORT=1212 && next dev --turbo",
    "dev:hot": "nodemon --watch . --exec 'npm run dev'",
    "build": "next build",
    "start": "cross-env PORT=1212 NODE_ENV=production next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "clean": "rm -rf .next node_modules && npm install"
  },
  "dependencies": {
    "@paypal/react-paypal-js": "^8.8.1",
    "axios": "^1.7.9",
    "cookies-next": "^5.1.0",
    "cross-env": "^7.0.3",
    "json2csv": "^6.0.0-alpha.2",
    "lottie-react": "^2.4.1",
    "lucide-react": "^0.475.0",
    "next": "^15.1.7",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-icons": "^5.4.0",
    "react-lottie-player": "^2.1.0",
    "turbopack": "^0.0.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/json2csv": "^5.0.7",
    "@types/node": "22.10.2",
    "@types/react": "19.0.2",
    "eslint": "^9",
    "eslint-config-next": "15.1.0",
    "postcss": "^8",
    "tailwindcss": "^3.4.1"
  }
}
